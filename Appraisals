(1..4).each do |minor|
  appraise "sinatra-1.#{minor}" do 
    gem 'sinatra', "1.#{minor}.0"
  end
end

appraise 'sinatra-0.9.4' do 
  gem 'sinatra', '0.9.4'
end

appraise 'sequel-3.2' do 
  gem 'sequel', '3.2'
end

appraise 'sequel-4.0' do 
  gem 'sequel', '4.0'
end

appraise 'sequel-4.17' do 
  gem 'sequel', '4.17'
end